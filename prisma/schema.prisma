generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model notes {
  id      String  @id
  owner   String?
  title   String?
  content Json?
}

model users {
  id       String  @id
  username String?
  password String?
  email    String?
}
